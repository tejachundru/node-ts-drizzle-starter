{
  "name": "ts-node-express",
  "version": "1.0.0",
  "description": "",
  "main": "dist/main.js",
  "scripts": {
    "db:generate": "npx drizzle-kit generate",
    "db:migrate": "npx drizzle-kit migrate",
    "run-studio": "drizzle-kit studio",
    "clean": "rimraf dist coverage",
    "typecheck": "tsc --noEmit",
    "lint": "npx eslint '*/**/*.{js,jsx,ts,tsx}' --quiet --fix",
    "prebuild": "npm-run-all clean lint typecheck",
    "build": "npx swc src -d dist --strip-leading-paths",
    "build:ts": "tsc && tsc-alias",
    "start": "nodemon --exec node ./dist/main.js",
    "dev": "npm-run-all build start",
    "dev:watch": "concurrently \"npm run watch-compile\" \"npm run watch-dev\"",
    "watch-compile": "npx swc src -w --out-dir dist --strip-leading-paths",
    "watch-dev": "nodemon --watch \"dist/**/*\" -e js ./dist/main.js",
    "deploy:pm2": "npm run build && npx pm2 reload ecosystem.config.js",
    "serve:staging": "NODE_ENV=staging node ./dist/main.js",
    "serve:production": "NODE_ENV=production node ./dist/main.js",
    "serve:staging-docker": "NODE_ENV=staging pm2-runtime ./dist/main.js",
    "serve:production-docker": "NODE_ENV=production pm2-runtime ./dist/main.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.758.0",
    "@aws-sdk/s3-request-presigner": "^3.758.0",
    "argon2": "^0.41.1",
    "bcrypt": "^5.1.1",
    "colorette": "^2.0.20",
    "compression": "^1.7.5",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
    "drizzle-orm": "^0.41.0",
    "drizzle-zod": "^0.7.0",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "express-useragent": "^1.0.15",
    "fastify": "^5.2.1",
    "helmet": "^8.0.0",
    "hpp": "^0.2.3",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.10.0",
    "openai": "^4.86.1",
    "pg": "^8.14.1",
    "pg-error-enum": "^0.7.3",
    "pino": "^9.6.0",
    "pino-http": "^10.4.0",
    "pino-pretty": "^13.0.0",
    "pm2": "^5.4.3",
    "redoc-express": "^2.1.0",
    "request-ip": "^3.3.0",
    "ts-alias": "^0.0.7",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@faker-js/faker": "^9.5.0",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.15",
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-rate-limit": "^6.0.2",
    "@types/express-useragent": "^1.0.5",
    "@types/hpp": "^0.2.6",
    "@types/jsonwebtoken": "^9.0.8",
    "@types/lodash": "^4.17.15",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.12.7",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.17",
    "@types/pg": "^8.11.11",
    "@types/request-ip": "^0.0.41",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "concurrently": "^9.1.2",
    "drizzle-kit": "^0.30.6",
    "eslint": "^9.19.0",
    "express-openapi-validator": "^5.4.9",
    "globals": "^15.14.0",
    "nodemon": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "redoc": "^2.4.0",
    "rimraf": "^6.0.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.3",
    "typescript": "^5.4.5",
    "typescript-eslint": "^8.23.0"
  },
  "modulePaths": [
    "src"
  ],
  "moduleNameMapper": {
    "^~(.*)$": "<rootDir>/src$1"
  }
}
